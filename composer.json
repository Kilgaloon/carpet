{
    "name": "kilgaloon/carpet",
    "description": "Migrate one database to another, even if they are different. Map tables and columns.",
    "type": "library",
    "require": {
        "illuminate/database": "^5.6",
        "illuminate/console": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Strahinja Djuric",
            "email": "radju22@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Carpet\\": "src/",
            "Models\\": "models/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    }, 
    "minimum-stability": "dev"
}
